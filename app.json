{
  "name": "StudioBot",
  "description": "A helpful bot for creative classrooms",
  "repository": "https://github.com/howdyai/botkit-starter-slack",
  "keywords": ["node", "bots", "slack","botkit", "studio"],
  "website": "https://studiobot.glitch.com",
  "success_url":"/",
  "addons":[
    {
      "plan": "mongolab",
      "as": "MONGO"
    }
  ],
  "env": {
      "clientId": {
          "description": "Client ID provided by Slack"
      },
      "clientSecret": {
          "description": "Client Secret provided by Slack"
      },
      "studio_token": {
          "description": "Token associated with bot in Botkit Studio",
          "required":false
      }
  }
}
